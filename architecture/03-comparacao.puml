@startuml comparacao
!theme cerulean-outline
skinparam backgroundColor #0a192f
skinparam defaultFontColor #ffffff
skinparam NoteBackgroundColor #1a365d
skinparam NoteBorderColor #d4af37

title <font color="#d4af37" size="24">Consultoria vs Resolvedor</font>

rectangle "CONSULTORIA TRADICIONAL" #1a365d {
  (Semana 1: Diagnostico) as c1
  (Semana 2: Proposta) as c2
  (Semana 3: Revisao) as c3
  (Semana 4: Contrato) as c4
  (Semana 5-12: Desenvolvimento) as c5
  (Semana 13+: Ajustes infinitos) as c6

  c1 --> c2
  c2 --> c3
  c3 --> c4
  c4 --> c5
  c5 --> c6
}

rectangle "RESOLVEDOR" #0a192f {
  (Semana 1: Sistema no ar) as r1 #d4af37
  (Semana 2: Usuarios testando) as r2 #d4af37
  (Semana 3: Ajustes em producao) as r3 #d4af37
  (Semana 4: Segunda feature) as r4 #d4af37
  (Problema resolvido) as r5 #d4af37

  r1 --> r2
  r2 --> r3
  r3 --> r4
  r4 --> r5
}

@enduml
